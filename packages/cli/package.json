{
  "name": "@query-morph/cli",
  "version": "0.1.3",
  "description": "CLI tool for query-morph - transform structural data from the command line.",
  "type": "module",
  "bin": {
    "query-morph": "./bin/query-morph.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --clean",
    "dev": "tsup src/index.ts --format esm --watch",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "query-morph",
    "cli",
    "transform",
    "json",
    "xml"
  ],
  "author": "Hyperwindmill",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hyperwindmill/query-morph.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/Hyperwindmill/query-morph/issues"
  },
  "homepage": "https://github.com/Hyperwindmill/query-morph/tree/main/packages/cli#readme",
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@query-morph/core": "*",
    "commander": "^13.1.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
